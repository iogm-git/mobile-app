{
  "extends": "@react-native/typescript-config/tsconfig.json",
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@root/*": [
        "./src/*"
      ],
      "@svg/*": [
        "./assets/svg/*"
      ],
      "@image/*": [
        "./assets/image/*"
      ],
      "@env": [
        ".env"
      ]
    }
  },
  "include": [
    "src",
    "assets/svg",
    "assets/image",
    ".env"
  ]
}